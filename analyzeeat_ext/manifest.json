{
  "manifest_version": 3,
  "name": "AnalyzeEat- AI Nutritionist",
  "version": "1.0",
  "description": "AI Nutritionist Assistant",
  "permissions": [
    "activeTab",
    "scripting",
    "sidePanel"
  ],
  "action": {
    "default_title": "Nutritionist Assistant"
  },
  "side_panel": {
  "default_path": "sidepanel.html"
},
"background":{
"service_worker": "background.js"
},
  "host_permissions": [
    "http://localhost:8080/*",
    "<all_urls>"
  ],
  "content_security_policy":{
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}